generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["groupBy"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model cars {
  id         Int     @id
  make       String?
  model      String?
  year       Int?
  fuelType   String?
  kilometers Int?
  details    String?
  price      Int?
  photoUrl   String?
}

model faq {
  id       Int     @id
  question String?
  answer   String?
}
